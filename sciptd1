set style data histogram 
set style fill solid

set boxwidth 0.75
set xrange[0.5:*]
set yrange [0:*]
set style histogram gap 5

set ylabel "chauffeurs"
set xlabel "Longueurs du trajet"
set title "Trajet les plus longs"


set offsets 0,0,0.5,0.5


set terminal png 
set output 'graphiqued1.png'
plot "traitd1" using 1:ytic(2) with boxes lc rgb "skyblue"
convert -rotate 90 graphiqued1.png graph.png
